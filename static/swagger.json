{
    "openapi": "3.0.1",
    "info": {
        "title": "Middleware Integração Imobiliária",
        "summary": "Middleware...",
        "description": "Esse Middleware é utilizado para salvar dados de diversas imobiliárias...",
        "contact": {
            "email": "csdamo@ucs.br"
        },
        "version": "1.0.0"
    },
    "externalDocs": {
        "description": "Relatório",
        "url": "https://docs.google.com/document/d/1i2txTpmApRxXQJ_6k5J-eZmU5q_L64zft-BC5AImaQI/edit?usp=drive_link"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:5000",
            "description": "Ambiente local"
        }
    ],
    "tags": [
        {
            "name": "imobiliaria",
            "description": "Unidades de negócio parceiras"
        },
        {
            "name": "imovel",
            "description": "Imóveis negociados"
        }
    ],
    "paths": {
        "/hello_world": {
            "post": {
                "tags": [
                    "imobiliaria"
                ],
                "summary": "Adiciona uma nova imobiliária",
                "description": "",
                "operationId": "addImobiliaria",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "Imobiliária a ser adicionada",
                        "required": true,
                        "schema": {
                            "$ref": "#/components/schemas/Imobiliaria"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Imobiliária criada com sucesso"
                    },
                    "400": {
                        "description": "Requisição inválida"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Imobiliaria": {
                "type": "object",
                "required": [
                    "nome_fantasia",
                    "cnpj",
                    "telefone",
                    "email"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "nome_fantasia": {
                        "type": "string",
                        "uniqueItems": true,
                        "example": "Imobiliária Exemplo"
                    },
                    "razao_social": {
                        "type": "string",
                        "nullable": true,
                        "example": "Razão Social Exemplo"
                    },
                    "cnpj": {
                        "type": "string",
                        "uniqueItems": true,
                        "example": "0000000000000"
                    },
                    "telefone": {
                        "type": "string",
                        "example": "0000000000"
                    },
                    "email": {
                        "type": "string",
                        "example": "email@exemplo.com"
                    }
                }
            }
        }
    }
}